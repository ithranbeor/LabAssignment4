{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "apk": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "environment": "preview"
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "android": {},
      "ios": {}
    }
  }
}
